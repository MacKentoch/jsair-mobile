{
  "name": "jsair",
  "version": "1.0.1",
  "private": true,
  "author": "Erwan DATIN (MacKentoch)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MacKentoch/jsair-mobile/issues"
  },
  "description": "javascript air mobile application",
  "keywords": [
    "javascriptair",
    "javascript",
    "air",
    "mobile",
    "app",
    "React",
    "native",
    "Native",
    "react",
    "redux",
    "starter",
    "es6",
    "ES6",
    "ES2015",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MacKentoch/jsair-mobile.git"
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "rm -rf ./node_modules/jest-cli/.haste_cache && jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/react-addons-test-utils/"
    ],
    "verbose": true,
    "collectCoverage": true
  },
  "dependencies": {
    "deindent": "0.1.0",
    "marked": "0.3.5",
    "moment": "2.13.0",
    "react": "0.14.8",
    "react-native": "0.25.1",
    "react-native-animatable": "0.6.0",
    "react-native-dismiss-keyboard": "0.0.3",
    "react-native-image-progress": "0.4.0",
    "react-native-progress": "2.2.2",
    "react-native-scrollable-tab-view": "0.4.1",
    "react-native-side-menu": "0.18.0",
    "react-native-vector-icons": "1.3.4",
    "react-redux": "4.4.2",
    "redux": "3.4.0",
    "redux-thunk": "2.0.1",
    "validator": "5.2.0"
  },
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "babel-jest": "^12.0.2",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "2.9.0",
    "eslint-plugin-react": "5.0.1",
    "eslint-plugin-react-native": "1.0.2",
    "jest-cli": "^12.0.2",
    "react-addons-test-utils": "^15.0.2",
    "react-native-mock": "^0.2.3",
    "redux-mock-store": "^1.1.1"
  }
}
